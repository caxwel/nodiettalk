{
    "name": "No Diet Talk",
    "version": "1.0",
    "description": "for searching recipe blogs: replaces diet talk with nature emoji",
    "manifest_version": 2,
    "icons": {
        "48": "icon1.png",
        "96": "icon2.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["wordmap.js", "replace.js"],
            "run_at": "document_end"
        }
    ]
  }